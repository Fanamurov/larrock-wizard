{
    "name": "fanamurov/larrock-wizard",
    "description": "Import .xlsx price to catalog component for larrockCMS",
    "type": "library",
    "license": "CC-BY-4.0",
    "authors": [
        {
            "name": "Alexandr Fanamurov",
            "email": "fanamurov@ya.ru"
        }
    ],
    "require": {
        "fanamurov/larrock-catalog": "^1.0",
        "maatwebsite/excel": "^2.1",
        "laracasts/generators": "^1.1"
    },
    "require-dev": {
        "laravel/framework": "5.6.*",
        "proengsoft/laravel-jsvalidation": "^2.0",
        "davejamesmiller/laravel-breadcrumbs": "5.*",
        "nicolaslopezj/searchable": "^1.9",
        "spatie/laravel-medialibrary": "^7.0.0",
        "phpunit/phpunit" : "^6.2|^7.0",
        "mockery/mockery": "^1.0.0",
        "orchestra/testbench": "~3.5.0|~3.6.0",
        "doctrine/dbal": "^2.5.2",
        "fanamurov/larrock-core": "^1.0"
    },
    "autoload": {
        "psr-4": {
            "Larrock\\ComponentWizard\\": "src"
        }
    },
    "autoload-dev": {
        "classmap": [
            "tests/"
        ],
        "psr-4": {
            "Larrock\\ComponentWizard\\Tests\\": "tests"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Larrock\\ComponentWizard\\LarrockComponentWizardServiceProvider"
            ]
        }
    }
}